{
  "version": "2.0.0",
  "manifest_version": 3,
  "name": "Tab menu and search history",
  "description": "View and search through open tabs and browser history.",
  "action": {
      "default_icon": {
        "19": "img/logo32x.png",
        "38": "img/logo32x.png"
      },
      "default_popup": "js/index.html",
      "default_title": "TabStacks [CTRL+SHIFT+K]"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "windows": "Ctrl+Shift+K",
        "mac": "MacCtrl+Shift+K"
      }
    }
  },
  "icons": {
    "16": "img/logo32x.png",
    "48": "img/logo64x.png",
    "64": "img/logo64x.png",
    "128": "img/logo128x.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "history"
  ],
  "content_security_policy": {}
}